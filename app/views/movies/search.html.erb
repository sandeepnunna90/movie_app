<ul class="flex-container wrap">
  <% @movies.each do |movie| %>
    <li class="flex-item">
        <div class="thumbnail m-5">
            <%= link_to (image_tag movie.image.url(:medium), class: 'image'), movie %>
        </div>
        <div>
          <%= movie.title %>
        </div>
    </li>
  <% end %>
</ul>
